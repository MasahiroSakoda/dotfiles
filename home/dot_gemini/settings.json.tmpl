{
  {{- /* https://geminicli.com/docs/get-started/configuration/ */}}
  {{/* Tool Settings */ -}}
  "tools": {
    "autoAccept": false,
    "core": [
      "LSTool",
      "GlobTool",
      "GrepTool",
      "ReadFileTool",
      "WriteFileTool",
      "EditTool",
      "FindFilesTool",
      "SearchTextTool",
      "ReadFolderTool",
      "ShellTool",
      "GoogleSearchTool",
      "WebFetchTool",
      "SaveMemoryTool"
    ],
    "exclude": [
      "run_shell_command(rm -rf)"
    ],
  {{/* Sandbox Settings */ -}}
  "sandbox": false
  },
  {{/* General Settings */ -}}
  "general": {
    "checkpointing": {
      "enabled": true
    },
    "preferredEditor": "neovim",
    "vimMode": false,
    "previewFeatures": true
  },
  "context": {
    "fileName": [
      "GEMINI.md",
      "AGENTS.md"
    ],
    {{/* File Filter Settings */ -}}
    "fileFiltering": {
      "respectGitIgnore": true,
      "enableRecursiveFileSearch": true
    }
  },
  {{/* Model Config */ -}}
  "model": {
    "name": "auto-gemini-3"
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "ui": {
    "theme": "GitHub"
  },
  "privacy": {
    "usageStatisticsEnabled": false
  },
  {{/* Telemetry */ -}}
  "telemetry": {
    "enabled": false,
    "target": "local",
    "otlpEndpoint": "http://localhost:4317",
    "logPrompts": true
  }
}
{{- /*
// -*-mode:jsonc-*- vim:ft=jsonc.gotexttmpl
*/ -}}
